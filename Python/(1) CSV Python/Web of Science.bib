@article{ WOS:000749400600001,
Author = {Udo, Fidelis and Naidu, Maheshvari},
Title = {Examining the Interplay between Economic Development and Local Women
   Vulnerability to Flood Impacts in Selected Local Areas in Durban, South
   Africa},
Journal = {JOURNAL OF ASIAN AND AFRICAN STUDIES},
Year = {2023},
Volume = {58},
Number = {5},
Pages = {779-793},
Month = {AUG},
Abstract = {This article probes the rural economic development approach in selected
   informal settlements in Durban and how such approach affects the
   vulnerability of local Black women to flood impacts within the areas.
   Qualitative data for the study were gathered through semi-structured
   interviews and focus group discussions with 25 local Black women from
   Inanda, Ntuzuma, KwaMashu and Umlazi. Five key informants from the
   eThekwini (Durban) metropolitan municipality were also interviewed.
   Findings from the study showed that although there is improved economic
   development in the selected settlements, which constitute informal
   settlements designated for Black South Africans during the apartheid
   era, such an economic development approach has not significantly
   improved the livelihoods and adaptive capacity of the local women. The
   article suggests a multidimensional approach to development that is
   practical, inclusive and equitable, and addresses local women's
   challenges associated with climate adaptation and sustainable
   livelihoods.},

@article{ WOS:001088224200003,
Author = {Echeverria, Carolina Castillo},
Title = {CLIMATE GOVERNANCE IN SMALL AGRICULTURAL FARMS IN THREE COSTA RICAN
   TOWNS FROM A GENDER PERSPECTIVE},

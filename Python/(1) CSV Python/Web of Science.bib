@article{ WOS:000524233600002,
Author = {Goli, Imaneh and Najafabadi, Maryam Omidi and Lashgarara, Farhad},
Title = {Where are We Standing and Where Should We Be Going? Gender and Climate
   Change Adaptation Behavior},
Journal = {JOURNAL OF AGRICULTURAL \& ENVIRONMENTAL ETHICS},

@article{ WOS:000320573900005,
Author = {Ketlhoilwe, Mphemelang Joseph},
Title = {Improving resilience to protect women against adverse effects of climate
   change},
Journal = {CLIMATE AND DEVELOPMENT},
Year = {2013},
Volume = {5},
Number = {2},
Pages = {153-159},
Month = {APR 1},
Abstract = {Rural women in Botswana are predominantly dependent on subsistence
   agriculture and natural products from the wild (natural environment).
   The effects of climate change and variability affect their sources of
   livelihoods. Over the years, through experience and interaction women
   have developed resilience to climate variability and change. Through
   government policy implementation and traditional knowledge, women have
   managed to protect themselves against adverse effects of climate change.
   This paper investigates how rural women have developed resilience to
   climate change and variability. The emerging main adaptive strategies
   used by women are social interaction (and social learning) and the
   integration of local knowledge and new technologies to improve on their
   resilience to climate change.},

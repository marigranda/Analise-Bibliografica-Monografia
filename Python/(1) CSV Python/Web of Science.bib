Author = {Prakash, Anjal and McGlade, Katriona and Roxy, Mathew Koll and Roy,
   Joyashree and Some, Shreya and Rao, Nitya},
Title = {Climate Adaptation Interventions in Coastal Areas: A Rapid Review of
   Social and Gender Dimensions},

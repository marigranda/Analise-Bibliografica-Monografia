@article{ WOS:000844158200002,
Author = {Aryal, Jeetendra Prakash and Sapkota, Tek Bahadur and Rahut, Dil Bahadur
   and Gartaula, Hom Nath and Stirling, Clare},
Title = {Gender and climate change adaptation: A case of Ethiopian farmers},
